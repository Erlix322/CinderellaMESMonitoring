<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:id="innerPaneEntityView" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="100.0" minWidth="200.0" xmlns="http://javafx.com/javafx/8">
    <children>
        <VBox alignment="CENTER" spacing="10.0">
            <children>
                <Label fx:id="label" text="Label">
                    <contextMenu>
                        <ContextMenu>
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#showHist" text="Show History"/>
                                <MenuItem mnemonicParsing="false" onAction="#scheduleEvent" text="schedule event"/>
                                <MenuItem mnemonicParsing="false" onAction="#showDisp" text="show Dispatchlist"/>
                            </items>
                        </ContextMenu>
                    </contextMenu>
                </Label>
                <Label fx:id="state" text="Label"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </VBox>
    </children>
</VBox>
